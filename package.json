{
  "name": "@pikselpalette/sequoia-js-client-sdk",
  "version": "1.0.0",
  "description": "Sequoia client SDK for Javascript",
  "main": "dist/web/sequoia-client.js",
  "scripts": {
    "prepublishOnly": "NODE_ENV=production webpack --bail --progress --profile --optimize-minimize",
    "build": "webpack --bail --progress --profile",
    "doc": "jsdoc -c jsdoc.conf.json -r lib -p -R README.md -P package.json --verbose --pedantic",
    "lint": "eslint lib && eslint test/spec",
    "test": "NODE_ENV=test jest --coverage && codecov",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:mutate": "NODE_ENV=test stryker run"
  },
  "jest": {
    "testMatch": [
      "**/test/spec/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.stryker-tmp/"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "setupFiles": [
      "./test/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pikselpalette/sequoia-js-client-sdk.git"
  },
  "keywords": [
    "sequoia",
    "es6",
    "javascript",
    "sdk"
  ],
  "author": "Piksel Product",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "codecov": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.0",
    "fetch-mock": "^6.3.0",
    "ink-docstrap": "^1.3.2",
    "jest": "^22.4.3",
    "jest-fetch-mock": "^1.5.0",
    "jsdoc": "^3.5.5",
    "stryker": "^0.22.1",
    "stryker-api": "^0.16.0",
    "stryker-babel-transpiler": "^0.4.1",
    "stryker-html-reporter": "^0.13.2",
    "stryker-javascript-mutator": "^0.6.2",
    "stryker-jest-runner": "^0.6.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "date-fns": "^1.29.0",
    "lodash.delay": "^4.1.1",
    "query-string": "^6.0.0"
  },
  "publishConfig": {
    "registry": "https://npmjs.org"
  }
}
